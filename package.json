{
  "name": "border-countdown-monorepo",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm -C apps/chat dev\" \"pnpm -C apps/web dev\"",
    "build": "pnpm -C apps/chat build && pnpm -C apps/web build",
    "start": "pnpm -C apps/chat start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
